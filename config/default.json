{
  "commentUser" :{
    "auth": {"user": "COMMENT_USER", "pass": "COMMENT_PASSWORD"},
    "user": "COMMENT_USER"
  },
  "sites": {
    "default": {
      "user": "SITE_USER",
      "repo": "SITE_REPO",
      "branch": "SITE_BRANCH",
      "urlMatchRegexp": ".*\\/(\\d+\\/\\d+\\/\\d+\\/.*)\\/$",
      "prefix": "_posts/",
      "suffix": ".markdown",
      "commentsEndMarker": "COMMENTS_END"
    }
  },
  "markdownCommentSyntax": "<!--- <%= data %> -->",
  "commentTemplate": "Comment by <%- metadata.name %> on <%- moment.format('lll') %>\n--------\n<%- comment %>\n"
}
